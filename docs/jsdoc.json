{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc"
    ]
  },
  "recurseDepth": 10,
  "source": {
    "include": [
      "src/gis-map/bus"
    ],
    "includePattern": ".+\\.(vue|js|ts)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "./docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": true,
    "default": {
      "outputSourceFiles": true,
      "includeDate": false,
      "useLongnameInNav": false,
      "systemName": "Common Modules",
      "footer": "",
      "navType": "vertical",
      "layoutFile": "plugins/layout.tmpl"
    }
  },
  "plugins": [
    "plugins/jsdoc-vue",
    "plugins/delFooter.js"
  ],
  "docdash": {
    "search": true,
    "collapse": true,
    "footer": "",
    "meta": {
      "title": "函数工具库",
      "description": "日常 JavaScript  函数工具库，方便查看",
      "keyword": "javaScript, utils, jsdoc"
    },
    "menu": {
      "语雀": {
        "href": "https://www.yuque.com/ever2/frontend/wrw0xt",
        "class": "menu-item",
        "id": "website_link"
      }
    }
  }
}